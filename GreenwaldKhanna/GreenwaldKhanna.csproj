    <Authors>Adam Shirey</Authors>
    <Product>GreenwaldKhanna.NET</Product>
    <PackageId>GreenwaldKhanna.NET</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Greenwald-Khanna Quantile Estimator</Description>
